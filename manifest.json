{
   "background": {
      "scripts": [ "idInfo.js", "utils.js", "logger.js", "cache.js", "config.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "images/ucc-19-bw.png"
      },
      "default_title": "Umbrella Chromebook client"
   },
   "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
   "default_locale": "en_US",
   "description": "Cisco Umbrella Chromebook client protects users by blocking malicious requests and helps admins to filter in-appropriate domains.",
   "icons": {
      "128": "images/ucc-128.png",
      "16": "images/ucc-16.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "name": "Cisco Umbrella Chromebook client (Ext)",
   "permissions": [ "webRequest", "alarms", "storage", "webRequestBlocking", "enterprise.deviceAttributes", "http://*/*", "https://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3.4",
   "version_name": "v1.3.4",
   "web_accessible_resources": [ "blocked.html" ]
}
